# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/dental_portal

# Keycloak
KEYCLOAK_URL=http://localhost:8180
KEYCLOAK_REALM=dental-portal
KEYCLOAK_CLIENT_ID=dental-portal-backend
KEYCLOAK_CLIENT_SECRET=your-client-secret

# Guacamole
GUACAMOLE_URL=http://localhost:8080/guacamole

# Windows Server (WinRM)
WINRM_HOST=192.168.1.100
WINRM_USER=administrator
WINRM_PASSWORD=your-winrm-password

# DICOM watcher
DICOM_WATCH_DIR=/mnt/dicom-export

# Session limits
SESSION_IDLE_TIMEOUT=900
SESSION_HARD_TIMEOUT=3600
MAX_CONCURRENT_SESSIONS=5
